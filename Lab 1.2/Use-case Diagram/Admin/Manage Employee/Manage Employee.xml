@startuml
left to right direction
skinparam packageStyle rectangle

actor Admin as A
rectangle "Manage Employee" {
  usecase (Manage Employee) as UC1
  usecase (Update Information) as UC2
  usecase (Add New Employee) as UC3
  usecase (Remove Employee) as UC4
}

A -- UC1

UC2 .> UC1 : extend
UC3 .> UC1 : extend
UC4 .> UC1 : extend
@enduml
