@startuml

actor User as U
rectangle "Watch Order History" {
  usecase (Request Order History) as UC1
  usecase (Retrieve Order History) as UC2
  usecase (Display Order History) as UC3
}

U --> UC1 : Requests Order History
UC1 --> UC2 : Initiates Order History Retrieval
UC2 --> UC3 : Retrieves and Processes Order History
UC3 --> U : Displays Order History
@enduml